##Open GUI if set to 10
execute as @s[scores={4j.gamecfg=10}] run function 4jbattle:menu/load/host/mapgui

##Disable Crucible if set to 11
execute as @s[scores={4j.gamecfg=11}] run function 4jbattle:menu/load/host/map/crucible/disable

##Enable Crucible if set to 12
execute as @s[scores={4j.gamecfg=12}] run function 4jbattle:menu/load/host/map/crucible/enable

##Disable Crucible if set to 13
execute as @s[scores={4j.gamecfg=13}] run function 4jbattle:menu/load/host/map/cove/disable

##Enable Cove if set to 14
execute as @s[scores={4j.gamecfg=14}] run function 4jbattle:menu/load/host/map/cove/enable

##Disable Cavern if set to 15
execute as @s[scores={4j.gamecfg=15}] run function 4jbattle:menu/load/host/map/cavern/disable

##Enable Cavern if set to 16
execute as @s[scores={4j.gamecfg=16}] run function 4jbattle:menu/load/host/map/cavern/enable